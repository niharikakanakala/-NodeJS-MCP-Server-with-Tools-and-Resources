{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "nodemon index.js",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "sample_tests": "jest --config jest.config.cjs --testNamePattern='sample' --forceExit --testTimeout=10000 --verbose --ci --testResultsProcessor=\"jest-junit\"",
    "main_tests": "jest --config jest.config.cjs tests/main.test.js --forceExit --testTimeout=10000 --verbose --ci --testResultsProcessor=\"jest-junit\""
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "0.4.0",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.17.1",
    "helmet": "7.1.0",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "jest": "26.6.3",
    "jest-junit": "12.0.0",
    "nodemon": "2.0.15",
    "supertest": "6.1.3",
    "text-encoding": "0.7.0"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": ".",
    "outputName": "xunittest-report.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  }
}